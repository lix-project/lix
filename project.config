[access]
	inheritFrom = Private-Projects
[submit]
[project]
	description = lix
[access "refs/*"]
	owner = group @lix-project
	owner = group user/RaitoBezarius
	owner = group user/puckipedia
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group @lix-project
	label-Code-Review = -2..+2 group user/RaitoBezarius
	label-Code-Review = -2..+2 group user/puckipedia
	label-Verified = -1..+1 group @lix-project
	label-Verified = -1..+1 group user/RaitoBezarius
	label-Verified = -1..+1 group user/puckipedia
	push = group @lix-project
	push = group user/RaitoBezarius
	push = group user/puckipedia
	read = group @lix-project
	read = group user/RaitoBezarius
	read = group user/puckipedia
	rebase = group @lix-project
	rebase = group user/RaitoBezarius
	rebase = group user/puckipedia
	removeReviewer = group @lix-project
	removeReviewer = group user/RaitoBezarius
	removeReviewer = group user/puckipedia
	submit = group @lix-project
	submit = group user/RaitoBezarius
	submit = group user/puckipedia
[access "refs/heads/*"]
	create = group @lix-project
	create = group user/RaitoBezarius
	create = group user/puckipedia
	push = group @lix-project
	push = group user/RaitoBezarius
	push = +force group user/puckipedia
	pushMerge = group @lix-project
	pushMerge = group user/RaitoBezarius
	pushMerge = group user/puckipedia
	read = group @lix-project
	read = group user/RaitoBezarius
	read = group user/puckipedia
[access "refs/tags/*"]
	push = group @lix-project
	push = group user/RaitoBezarius
	push = group user/puckipedia
	push = +force group user/puckipedia
